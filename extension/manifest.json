{
    "manifest_version": 2,
   
    "name": "DarkAlgo",
    "version": "0.1.0",
    "description": "Algo with Dark",
    "permissions": [
      "storage",
      "unlimitedStorage",
      "https://www.acmicpc.net/*"
    ],

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "icons": {
      "48": "icons/48.png",
      "96": "icons/96.png",
      "128": "icons/128.png",
      "1024": "icons/1024.png"
    }
   
   
  }