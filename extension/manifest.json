{
    "manifest_version": 2,
   
    "name": "DarkAlgo",
    "version": "0.1.0",
    "description": "Algo with Dark",
    "permissions": [
      "storage",
      "unlimitedStorage",
      "https://www.acmicpc.net/*"
    ],

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "browser_action": {
      "default_icon": "icon/white.png",
      "default_title": "DarkAlgo",
      "default_popup": "index.html"
    }

   
   
  }